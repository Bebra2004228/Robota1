#include <stdio.h>

long long countNumbers(int r) {
    // Ініціалізуємо змінні для підрахунку кількості чисел
    long long count = 0;
    long long countWithFive = 1;
    long long countWithNine = 1;
    long long temp;

    // Перебираємо кожен розряд
    for (int i = 2; i <= r; i++) {
        // Знаходимо кількість чисел, що закінчуються на 5
        temp = countWithFive;
        countWithFive += countWithNine;

        // Знаходимо кількість чисел, що закінчуються на 9
        countWithNine = temp;
    }

    // Загальна кількість чисел буде сумою кількостей з кожним розрядом
    count = countWithFive + countWithNine;

    return count;
}

int main() {
    int r;
    long long result;

    printf("Введіть кількість розрядів (р ≤ 30): ");
    scanf("%d", &r);

    // Викликаємо функцію для підрахунку кількості чисел
    result = countNumbers(r);

    printf("Кількість чисел із %d розрядами: %lld\n", r, result);

    return 0;
}  
